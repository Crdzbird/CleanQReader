{
  "expo": {
    "name": "CleanQReader",
    "slug": "CleanQReader",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Project used to read and store a list of QR Codes.\nStack used:\n- Redux-thunk.\n- Redux-saga\n- Hooks\n- Reducers\n- Navigator\n- Redux\n- ViewModels\n- Expo",
    "githubUrl": "https://github.com/Crdzbird/CleanQReader"
  }
}
